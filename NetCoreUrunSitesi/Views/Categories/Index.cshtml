@model Category

@{
    ViewData["Title"] = @Model.Name;
}

<div class="album">
    <h1 class="text-center">@Model.Name</h1>
    <div class="container">
        <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4 g-3">
            <partial name="_ProductPartial" model="Model.Products" />
        </div>
    </div>
</div>